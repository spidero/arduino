#include <dht.h>

// DHT11 Arduino 5V->5V
// DHT11 GND -> Arduino GND
// DHT11 Signal -> Arduino PIN D2

dht DHT;

#define DHT11_PIN 2

void setup(){
  Serial.begin(9600);
}

void loop(){
  //int chk = DHT.read11(DHT11_PIN);
  int chk = DHT.read11(2);
  Serial.print("Temperature = ");
  Serial.println(DHT.temperature);
  Serial.print("Humidity = ");
  Serial.println(DHT.humidity);
  delay(1000);
}
